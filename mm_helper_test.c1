#use <string>
#use <conio>



int main() 
{
    heap_t* h = heap_init(1024);
    obj_t o = make_full_block(h, 2, 3, true);
    write_to_obj(h, o, 10, 0);
    assert(h->heap_mem[3] == 10);
    write_to_obj(h, o, 11, 1);
    assert(h->heap_mem[4] == 11);
    printf("All tests passed. \n");
    return 0;
}